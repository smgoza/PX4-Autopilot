#!nsh
#
# @name Rotormast Osprey V-22
#
# @type Helicopter
# @class Copter
#
#
# @maintainer Mike Goza
#
# @output MAIN1 Left servo 1
# @output MAIN2 Left servo 2
# @output MAIN3 Left motor
# @output MAIN4 Left nacelle
# @output MAIN5 Right servo 1
# @output MAIN6 Right servo 2
# @output MAIN7 Right motor
# @output MAIN8 Right nacelle
#

sh /etc/init.d/rc.mc_defaults

param set MAV_TYPE 4
set MIXER rotormast_osprey

set PWM_OUT 12345678
param set PWM_RATE 50

param set PWM_MIN 900
param set PWM_MAX 2100
param set PWM_MAIN_DIS1 1500
param set PWM_MAIN_DIS2 1500
param set PWM_MAIN_DIS3 900
param set PWM_MAIN_DIS4 1000
param set PWM_MAIN_DIS5 1500
param set PWM_MAIN_DIS6 1500
param set PWM_MAIN_DIS7 900
param set PWM_MAIN_DIS8 2000

set HIL no
